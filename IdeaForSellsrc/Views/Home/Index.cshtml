@using IdeaForSellsrc.Models.ViewModel.ViewModel;
@model IndexViewModel
@{
    ViewBag.Title = "Index";
}

@{
    for (int i = 0; i < 9; i++)
    {
        <div class="col-md-4 mb">
            <!-- WHITE PANEL - TOP USER -->
            <div class="white-panel pn">
                <div class="white-header">
                    <h5>HashTags</h5>
                </div>
                <p><img src="assets/img/ui-zac.jpg" class="img-circle" width="80"></p>
                <p><b>Zac Snider</b></p>
                <div class="row">
                    <div class="col-md-6">
                        <p class="small mt">@Model.ListOfLiterals["RespondCount"]</p>
                        <p>2012</p>
                    </div>
                    <div class="col-md-6">
                        <p class="small mt">@Model.ListOfLiterals["ViewCount"]</p>
                        <p>$ 47,60</p>
                    </div>
                </div>
            </div>
        </div><!-- /col-md-4 -->
    }
}